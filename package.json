{
  "name": "kocsis-portal",
  "version": "0.0.0",
  "description": "A React port of the Gentelella Bootstrap admin template",
  "scripts": {
    "build": "webpack",
    "lint": "eslint --ext .js --ext .jsx .",
    "start": "webpack-dev-server --env demo",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NicolasT/gentelella-react.git"
  },
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ],
      "react"
    ],
    "plugins": [
      "transform-export-extensions"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "plugins": [
      "react"
    ],
    "env": {
      "browser": true
    }
  },
  "keywords": [
    "css",
    "js",
    "html",
    "template",
    "admin",
    "bootstrap",
    "theme",
    "backend",
    "react",
    "responsive"
  ],
  "author": "Nicolas Trangez",
  "contributors": [
    "Aigars Silkalns",
    "Christian Esperar"
  ],
  "license": "MIT",
  "main": "index.js",
  "bugs": {
    "url": "https://github.com/NicolasT/gentelella-react/issues"
  },
  "homepage": "https://github.com/NicolasT/gentelella-react#readme",
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "browser-sync": "^2.12.10",
    "css-loader": "^0.26.2",
    "eslint": "^3.16.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "file-loader": "^0.10.1",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-concat": "^2.6.0",
    "gulp-rename": "^1.2.2",
    "gulp-ruby-sass": "^2.0.6",
    "gulp-uglify": "^1.5.3",
    "html-webpack-plugin": "^2.28.0",
    "html-webpack-template": "^6.0.1",
    "node-sass": "^4.5.0",
    "sass-loader": "^6.0.2",
    "script-loader": "^0.7.0",
    "style-loader": "^0.13.2",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  },
  "dependencies": {
    "es6-promise": "^4.1.1",
    "isomorphic-fetch": "^2.2.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router-dom": "^4.1.1",
    "recharts": "^1.0.0-alpha.2",
    "styled-components": "^2.1.1"
  }
}
